{
  "engines": {
    "node": ">=8"
  },
  "name": "regression-analysis",
  "version": "0.1.0",
  "description": "Tool to analyse risk for regressions based on git diff",
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test",
      "...": "..."
    }
  },
  "main": "index.js",
  "scripts": {
    "docs": "node ./node_modules/.bin/jsdoc --configure .jsdocrc.json",
    "build": "node console.log('build');",
    "start": "node console.log('start');",
    "test": "node ./node_modules/.bin/jest --config=.jestrc.json",
    "lint": "node ./node_modules/.bin/eslint --config=.eslintrc.json src/**/**",
    "codecov": "node ./node_modules/.bin/codecov",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "precommit": "npm run test && npm run lint && npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vanils/regression-analysis.git"
  },
  "keywords": [
    "regression"
  ],
  "author": "Matti Mehtonen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vanils/regression-analysis/issues"
  },
  "homepage": "https://github.com/vanils/regression-analysis#readme",
  "dependencies": {
    "babylon": "^6.18.0",
    "minimist": "^1.2.0",
    "uniqid": "^4.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.2.0",
    "@commitlint/config-conventional": "^6.1.3",
    "codecov": "^3.0.1",
    "eslint": "^4.19.1",
    "eslint-plugin-jest": "^21.15.1",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "jsdoc": "^3.5.5",
    "please-upgrade-node": "^3.0.2",
    "semantic-release": "^15.1.8",
    "taffydb": "^2.7.3"
  }
}
